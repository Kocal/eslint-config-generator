{
  "name": "@yproximite/eslint-config-generator",
  "version": "0.0.0-development",
  "publishConfig": {
    "access": "public"
  },
  "description": "ESLint config generator for our internal and public projects. Allows to configure Vue, TypeScript, Prettier, ... plugins.",
  "repository": "https://github.com/Yproximite/eslint-config-generator.git",
  "author": "Hugo Alliaume <kocal@live.fr>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -fr dist",
    "build": "yarn build:js && yarn build:types",
    "build:js": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "pretest": "yarn run build",
    "test": "yarn test:ts && yarn test:unit",
    "test:ts": "tsc  --noEmit",
    "test:unit": "jest",
    "lint": "eslint . --ext .js,.ts"
  },
  "engines": {
    "node": "^10.13.0 || ^12.13.0 || >=14.0.0"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0"
  },
  "peerDependencies": {
    "@babel/eslint-parser": "7.0.0",
    "eslint": "^7.0.0",
    "prettier": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/eslint": "^7.2.4",
    "@types/jest": "^26.0.14",
    "eslint": "^7.11.0",
    "jest": "^26.6.0",
    "prettier": "^2.1.2",
    "rollup": "^2.32.0",
    "typescript": "^4.0.3"
  }
}
